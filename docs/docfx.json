{
  "metadata": [
    {
      "src": [
        {
          "src": "./..",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/*UnitTests.csproj"
          ]
        }
      ],
      "dest": "api",
      "namespaceLayout": "nested",
      "enumSortOrder": "declaringOrder"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "./custom-template"
    ],
    "globalMetadata": {
      "_appName": "FoggyBalrog.MermaidDotnet Documentation",
      "_appTitle": "FoggyBalrog.MermaidDotnet Documentation",
      "_appFooter": "See <a href='https://github.com/FoggyBalrog/MermaidDotnet' target='_blank'>FoggyBalrog.MermaidDotnet on GitHub</a>.",
      "_enableSearch": true,
      "_disableContribution": true,
      "pdf": false
    }
  }
}